{
  "kind": "collectionType",
  "collectionName": "cpanels",
  "info": {
    "name": "Cpanel",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "username": {
      "type": "string",
      "regex": "^[a-z]+$",
      "required": true,
      "maxLength": 16,
      "minLength": 6,
      "unique": true
    },
    "password": {
      "type": "password",
      "required": true,
      "minLength": 8
    },
    "email": {
      "type": "email",
      "required": true
    },
    "domain": {
      "type": "string",
      "regex": "^((?:([a-z0-9]\\.|[a-z0-9][a-z0-9\\-]{0,61}[a-z0-9])\\.)+)([a-z0-9]{2,63}|(?:[a-z0-9][a-z0-9\\-]{0,61}[a-z0-9]))\\.?$",
      "required": true,
      "unique": true
    },
    "client_name": {
      "type": "string",
      "required": true
    },
    "user_id": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "cpanels"
    },
    "server": {
      "model": "server"
    },
    "adminpanels": {
      "via": "cpanel",
      "collection": "adminpanels"
    }
  }
}
