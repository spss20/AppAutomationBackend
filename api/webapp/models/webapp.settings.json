{
  "kind": "collectionType",
  "collectionName": "webapps",
  "info": {
    "name": "Webapp"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "appname": {
      "type": "string",
      "required": true
    },
    "icon": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true
    },
    "package": {
      "type": "string",
      "required": true
    },
    "website": {
      "type": "string",
      "required": true
    },
    "version_name": {
      "type": "decimal"
    },
    "version_code": {
      "type": "integer"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user"
    }
  }
}
