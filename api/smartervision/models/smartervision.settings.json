{
  "kind": "collectionType",
  "collectionName": "smartervisions",
  "info": {
    "name": "Smartervision",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "appName": {
      "type": "string",
      "required": true
    },
    "baseUrl": {
      "type": "string",
      "required": true
    },
    "mapApi": {
      "type": "string"
    },
    "packageName": {
      "type": "text",
      "required": true
    },
    "icon": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true
    },
    "splashImage": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false
    },
    "googleJson": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "files"
      ],
      "plugin": "upload",
      "required": true
    },
    "versionCode": {
      "type": "integer"
    },
    "versionName": {
      "type": "string"
    },
    "appType": {
      "type": "enumeration",
      "enum": [
        "user",
        "vendor",
        "delivery"
      ],
      "required": true
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "smartervision"
    }
  }
}
