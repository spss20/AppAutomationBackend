    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "config": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "files"
      ],
      "plugin": "upload",
      "required": true
    },
    "outputUrl": {
      "type": "string"
    },
    "productSlug": {
      "type": "string",
      "private": false,
      "required": true
    },
    "orderId": {
      "type": "integer"
    }
  }
}
